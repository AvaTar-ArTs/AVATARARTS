{
  "name": "cleanconnect-pro-enhanced",
  "version": "2.0.0",
  "description": "Next-Generation AI-Powered Cleaning Marketplace with AR, Web3, and Content-Awareness",
  "main": "src/app.js",
  "scripts": {
    "dev": "concurrently \"yarn dev:api\" \"yarn dev:frontend\" \"yarn dev:ar\" \"yarn dev:web3\"",
    "dev:api": "cd backend && yarn dev",
    "dev:frontend": "cd frontend && yarn dev",
    "dev:ar": "cd ar-vr && yarn dev",
    "dev:web3": "cd web3 && yarn dev",
    "dev:mobile": "cd mobile && yarn dev",
    "dev:ai": "cd ai-services && python -m uvicorn main:app --reload",
    "dev:all": "concurrently \"yarn dev\" \"yarn dev:ai\"",
    "build": "yarn build:frontend && yarn build:ar && yarn build:web3",
    "build:frontend": "cd frontend && yarn build",
    "build:ar": "cd ar-vr && yarn build",
    "build:web3": "cd web3 && yarn build",
    "start": "pm2 start ecosystem.enhanced.config.js",
    "stop": "pm2 stop cleanconnect-enhanced",
    "restart": "pm2 restart cleanconnect-enhanced",
    "test": "yarn test:all",
    "test:all": "yarn test:api && yarn test:frontend && yarn test:ai",
    "test:api": "cd backend && yarn test",
    "test:frontend": "cd frontend && yarn test",
    "test:ai": "cd ai-services && python -m pytest",
    "test:ar": "cd ar-vr && yarn test",
    "test:web3": "cd web3 && yarn test",
    "test:coverage": "yarn test:coverage:api && yarn test:coverage:frontend",
    "test:coverage:api": "cd backend && yarn test:coverage",
    "test:coverage:frontend": "cd frontend && yarn test:coverage",
    "lint": "yarn lint:all",
    "lint:all": "yarn lint:api && yarn lint:frontend && yarn lint:ar && yarn lint:web3",
    "lint:api": "cd backend && yarn lint",
    "lint:frontend": "cd frontend && yarn lint",
    "lint:ar": "cd ar-vr && yarn lint",
    "lint:web3": "cd web3 && yarn lint",
    "lint:fix": "yarn lint:fix:all",
    "lint:fix:all": "yarn lint:fix:api && yarn lint:fix:frontend && yarn lint:fix:ar && yarn lint:fix:web3",
    "lint:fix:api": "cd backend && yarn lint:fix",
    "lint:fix:frontend": "cd frontend && yarn lint:fix",
    "lint:fix:ar": "cd ar-vr && yarn lint:fix",
    "lint:fix:web3": "cd web3 && yarn lint:fix",
    "migrate": "cd backend && yarn migrate",
    "migrate:create": "cd backend && yarn migrate:create",
    "migrate:up": "cd backend && yarn migrate:up",
    "migrate:down": "cd backend && yarn migrate:down",
    "seed": "cd backend && yarn seed",
    "db:reset": "cd backend && yarn db:reset",
    "db:backup": "cd backend && yarn db:backup",
    "ai:setup": "mamba env create -f environment.yml",
    "ai:activate": "mamba activate cleanconnect-pro-enhanced",
    "ai:update": "mamba env update -f environment.yml",
    "ai:train": "cd ai-services && python train_models.py",
    "ai:test": "cd ai-services && python test_ai_services.py",
    "ar:build": "cd ar-vr && yarn build:ar",
    "ar:test": "cd ar-vr && yarn test:ar",
    "web3:deploy": "cd web3 && yarn deploy:contracts",
    "web3:test": "cd web3 && yarn test:contracts",
    "content:generate": "cd ai-services && python generate_content.py",
    "content:trends": "cd ai-services && python analyze_trends.py",
    "social:post": "cd ai-services && python social_media_automation.py",
    "deploy": "yarn build && yarn migrate && yarn start",
    "deploy:staging": "yarn build && yarn migrate && pm2 start ecosystem.staging.config.js",
    "deploy:production": "yarn build && yarn migrate && pm2 start ecosystem.production.config.js",
    "monitor": "pm2 monit",
    "logs": "pm2 logs cleanconnect-enhanced",
    "docs": "cd docs && yarn serve",
    "setup:all": "yarn install && yarn ai:setup && yarn web3:deploy",
    "setup:mamba": "mamba env create -f environment.yml && echo 'âœ… Mamba environment created! Run: mamba activate cleanconnect-pro-enhanced'",
    "clean": "yarn clean:all",
    "clean:all": "yarn clean:node_modules && yarn clean:build && yarn clean:cache",
    "clean:node_modules": "find . -name 'node_modules' -type d -exec rm -rf {} +",
    "clean:build": "find . -name 'dist' -type d -exec rm -rf {} +",
    "clean:cache": "yarn cache clean",
    "install:yarn": "yarn install",
    "install:pnpm": "pnpm install",
    "install:bun": "bun install",
    "install:all": "yarn install",
    "dev:yarn": "yarn dev",
    "dev:pnpm": "pnpm dev",
    "dev:bun": "bun run dev",
    "build:yarn": "yarn build",
    "build:pnpm": "pnpm build",
    "build:bun": "bun run build"
  },
  "keywords": [
    "ai-powered",
    "cleaning",
    "marketplace",
    "ar",
    "vr",
    "web3",
    "blockchain",
    "content-aware",
    "trending",
    "viral",
    "social-media",
    "influencer",
    "nft",
    "defi",
    "dao",
    "cryptocurrency",
    "metaverse",
    "iot",
    "smart-home",
    "quantumforgelabs"
  ],
  "author": "Quantum Forge Labs <dev@quantumforgelabs.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quantumforgelabs/cleanconnect-pro-enhanced.git"
  },
  "bugs": {
    "url": "https://github.com/quantumforgelabs/cleanconnect-pro-enhanced/issues"
  },
  "homepage": "https://enhanced.cleanconnect.quantumforgelabs.org",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "socket.io": "^4.7.4",
    "stripe": "^14.7.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "openai": "^4.28.0",
    "groq-sdk": "^0.3.3",
    "pinecone-client": "^1.1.0",
    "supabase": "^1.123.4",
    "ethers": "^6.8.1",
    "web3": "^4.3.0",
    "@metamask/detect-provider": "^2.0.0",
    "three": "^0.158.0",
    "aframe": "^1.4.0",
    "ar.js": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next": "^14.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "date-fns": "^2.30.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "react-hot-toast": "^2.4.1",
    "axios": "^1.6.2",
    "socket.io-client": "^4.7.4",
    "graphql": "^16.8.1",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "bull": "^4.12.2",
    "node-cron": "^3.0.3",
    "sharp": "^0.32.6",
    "langchain": "^0.1.0"
  }
}
